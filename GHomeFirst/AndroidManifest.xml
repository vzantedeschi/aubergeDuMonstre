            android:name="com.example.ghomefirst.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ghomefirst.ReponseAlerte"
            android:label="@string/title_activity_reponse_alerte" >
        </activity>
        <activity
            android:name="com.example.ghomefirst.OuiManger"
            android:label="@string/title_activity_oui_manger" >
        </activity>
        <activity
            android:name="com.example.ghomefirst.NonManger"
            android:label="@string/title_activity_non_manger" >
        </activity>
    </application>

</manifest>
